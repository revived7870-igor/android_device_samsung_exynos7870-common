genfscon sysfs /devices/virtual/sec/sec_touchkey/brightness                                                             u:object_r:sysfs_backlight_writable:s0

# Touch
genfscon sysfs /devices/i2c@20/i2c-5/5-0020/input/input0/enabled                                                        u:object_r:sysfs_touch:s0
genfscon sysfs /devices/13890000.i2c/i2c-8/8-0050/input/input1/enabled                                                  u:object_r:sysfs_input_writable:s0

# GPS
genfscon sysfs /devices/soc0/machine                                                                                    u:object_r:sysfs_sec_gps:s0
genfscon sysfs /devices/soc0/revision                                                                                   u:object_r:sysfs_sec_gps:s0


# Graphics
genfscon sysfs /devices/14830000.decon_fb/vsync                                                                         u:object_r:sysfs_decon:s0
genfscon sysfs /devices/14830000.decon_fb/psr_info                                                                      u:object_r:sysfs_decon_writable:s0

# Revision
genfscon sysfs /firmware/devicetree/base/model_info-system_rev                                                          u:object_r:sysfs_chipid:s0

# RTC
genfscon sysfs /devices/10510000.hsi2c/i2c-0/0-a066/s2m-rtc/rtc/rtc0/hctosys                                            u:object_r:sysfs_rtc:s0

